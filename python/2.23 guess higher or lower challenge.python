import random  
  
highest=int(input("enter highest value \n"))  
guess=0  
answer=random.randint(1,highest)  
#print(answer)  
  
while guess!=answer:  
    guess=int(input("guess? \n"))  
    if guess==0:  
        break  
    if guess == answer:  
        print("correct1")  
        break  
    else:  
        if guess<answer:  
            print("guess higher\n")  
        else:  
            print("guess lower\n")
